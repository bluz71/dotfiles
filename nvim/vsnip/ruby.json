{
  "Class definition": {
    "prefix": "class",
    "body": [
      "class ${1:Class}",
      "  def initialize${2:()}",
      "    $3",
      "  end",
      "end"
    ]
  },
  "Initialize method": {
    "prefix": "defi",
    "body": [
      "def initialize${1:()}",
      "  $2",
      "end"
    ]
  },
  "Rails controller": {
    "prefix": "railsc",
    "body": [
      "# frozen_string_literal: true",
      "",
      "class ${1:Controller} < ApplicationController",
      "  def index",
      "    $2",
      "  end",
      "end"
    ]
  },
  "Rails model": {
    "prefix": "railsm",
    "body": [
      "# frozen_string_literal: true",
      "",
      "class ${1:Model} < ApplicationRecord",
      "  $2",
      "end"
    ]
  },
  "Individual spec": {
    "prefix": "it",
    "body": [
      "it \"$1\" do",
      "  $2",
      "end"
    ]
  }
}
